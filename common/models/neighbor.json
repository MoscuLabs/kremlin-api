{
  "name": "Neighbor",
  "plural": "Neighbors",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "first_name": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "string",
      "required": true
    },
    "last_name": {
      "type": "string",
      "required": true
    },
    "position": {
      "type": "string"
    },
    "gender": {
      "type": "string"
    },
    "representant": {
      "type": "boolean",
      "default": false
    },
    "isAllowedToPropose": {
      "type": "boolean",
      "default": false
    },
    "age": {
      "type": "number"
    },
    "profile_img": {
      "type": "string"
    },
    "created_at": {
      "type": "date",
      "default": "$now"
    },
    "birth_date": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "issues": {
      "type": "hasMany",
      "model": "Issue",
      "foreignKey": ""
    },
    "proposals": {
      "type": "hasMany",
      "model": "Proposal",
      "foreignKey": ""
    },
    "neighborhood": {
      "type": "belongsTo",
      "model": "Neighborhood",
      "foreignKey": ""
    },
    "votes": {
      "type": "hasMany",
      "model": "Vote",
      "foreignKey": ""
    },
    "expenses": {
      "type": "hasMany",
      "model": "Expense",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
