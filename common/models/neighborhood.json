      }
    },
    "administrators": {
      "type": "hasAndBelongsToMany",
      "model": "Administrator",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "issues": {
      "type": "hasMany",
      "model": "Issue",
      "foreignKey": ""
    },
    "proposals": {
      "type": "hasMany",
      "model": "Proposal",
      "foreignKey": ""
    },
    "boards": {
      "type": "hasMany",
      "model": "board",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
