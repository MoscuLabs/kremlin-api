{
  "name": "Neighborhood",
  "plural": "Neighborhoods",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "rules_file": {
      "type": "string",
      "default": ""
    },
    "location": {
      "type": "geopoint",
      "required": true
    },
    "created_at": {
      "type": "date",
      "default": "$now"
    }
  },
  "validations": [],
  "relations": {
    "neighbors": {
      "type": "hasMany",
      "model": "Neighbor",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "issues": {
      "type": "hasMany",
      "model": "Issue",
      "foreignKey": ""
    },
    "proposals": {
      "type": "hasMany",
      "model": "Proposal",
      "foreignKey": ""
    },
    "boards": {
      "type": "hasMany",
      "model": "board",
      "foreignKey": ""
    },
    "expenses": {
      "type": "hasMany",
      "model": "Expense",
      "foreignKey": ""
    },
    "request": {
      "type": "hasMany",
      "model": "Request",
      "foreignKey": ""
    },
    "notices": {
      "type": "hasMany",
      "model": "Notice",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
